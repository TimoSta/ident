{
  "manifest_version": 2,
  "name": "Ident",
  "author": "Timo M. Staudinger",
  "version": "1.0.0",
  "description": "A girl has no name.",
  "icons": {
    "16": "ident-16.png",
    "48": "ident-48.png",
    "128": "ident-128.png"
  },
  "browser_action": {
    "default_icon": "ident.png",
    "default_title": "Ident",
    "default_popup": "index.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "permissions": [
    "cookies",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ]
}
